{
    "name": "allekslar/monofony-menu-extension",
    "type": "symfony-bundle",
    "description": "Adds 'sylius.menu.admin.main' event for Monofony/Skeleton",
    "license": "MIT",
    "minimum-stability": "dev",
    "authors": [
        {
          "name": "Aleks",
          "email": "websiteservise@gmail.com"
        }
      ],
    "require": {
         "php": ">=8.0"
    },
    "require-dev": {
        "symfony/config": "^6.0",
        "symfony/dependency-injection": "^6.0",
        "symfony/http-kernel": "^6.0",
        "symfony/phpunit-bridge": "^6.0"
    },
    "autoload": {
        "psr-4": {
            "Allekslar\\MonofonyMenuBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Allekslar\\MonofonyMenuBundle\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    }
}
